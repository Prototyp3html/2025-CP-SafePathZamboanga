    "dependencies": {
        "core-js": "3"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.0",
        "@babel/core": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "babel-jest": "^24.8.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-plugin-import": "^2.18.0",
        "jest": "^24.8.0"
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?#": "babel-jest"
        }
    }
}
