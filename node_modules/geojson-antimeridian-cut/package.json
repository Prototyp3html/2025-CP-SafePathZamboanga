    "license": "MIT",
    "author": "Alex van de sandt <alex@avandesa.dev> (https://avandesa.dev)",
    "homepage": "https://gitlab.com/avandesa/geojson-antimeridian-cut/",
    "repository": "gitlab:avandesa/geojson-antimeridian-cut",
    "keywords": [
        "geojson",
        "json"
    ],
    "scripts": {
        "build": "babel src --out-dir lib",
