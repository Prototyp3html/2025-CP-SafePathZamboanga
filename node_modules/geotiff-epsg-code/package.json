{
  "name": "geotiff-epsg-code",
  "version": "0.3.1",
  "description": "Get the EPSG Code for a GeoTIFF.",
  "types": "index.d.ts",
  "type": "module",
  "main": "index.cjs",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "files": [
    "index.mjs",
    "index.cjs",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npx babel --plugins @babel/plugin-transform-modules-commonjs --plugins @babel/plugin-proposal-export-namespace-from index.mjs --out-file index.cjs && echo '\nmodule.exports = exports.default; module.exports.default = exports.default;' >> index.cjs",
    "format": "npx prettier --write index.mjs index.d.ts test.js",
    "prepublish": "npm run format && npm run build && npm test",
    "setup": "cd ./data && ./setup.sh",
    "test": "node test.js && node test.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeoTIFF/geotiff-epsg-code.git"
  },
  "keywords": [
    "crs",
    "esri",
    "geotiff",
    "projection",
    "srs"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/GeoTIFF/geotiff-epsg-code/issues"
  },
  "homepage": "https://github.com/GeoTIFF/geotiff-epsg-code#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "flug": "^2.6.0",
    "geotiff": "^2.0.7"
  },
  "dependencies": {
    "get-epsg-code": "^1.1.0"
  }
}
