{
  "name": "mediana",
  "version": "2.0.0",
  "description": "A Quick and Precise Library for Calculating the Median Value of an Array of Numbers (Typed or Untyped)",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "unpkg": "./dist/mediana.js",
  "jsdelivr": "./dist/mediana.js",
  "files": [
    "src/index.d.ts",
    "src/index.js",
    "src/count.js",
    "src/mean.js",
    "dist/mediana.js"
  ],
  "scripts": {
    "b": "npm run build",
    "build": "npx browserify ./src/index.js > ./dist/mediana.js",
    "cleanup": "npm uninstall median median-quickselect simple-statistics",
    "f": "npm run format",
    "format": "npx prettier --arrow-parens=avoid --print-width=200 --trailing-comma=none --write *.js src/*.js",
    "perf": "node perf.js",
    "setup": "npm install -D median median-quickselect simple-statistics",
    "test": "npx ts-node test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/mediana.git"
  },
  "keywords": [
    "fast",
    "faster",
    "fastest",
    "image",
    "integers",
    "math",
    "median",
    "no-data",
    "numbers",
    "pixels",
    "quick",
    "speed",
    "stats"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/mediana/issues"
  },
  "homepage": "https://github.com/DanielJDufour/mediana#readme",
  "devDependencies": {
    "flug": "^2.7.2",
    "median-quickselect": "^1.0.1",
    "simple-statistics": "^7.8.3"
  },
  "dependencies": {
    "preciso": "^0.12.2"
  }
}
