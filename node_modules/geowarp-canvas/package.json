  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/geowarp-canvas/issues"
  },
  "homepage": "https://github.com/DanielJDufour/geowarp-canvas#readme",
  "devDependencies": {
    "canvas": "^2.11.2",
    "flug": "^2.7.2",
    "geotiff": "1.0.9",
    "geotiff-epsg-code": "^0.3.1",
    "geotiff-no-data": "^0.2.0",
    "geowarp": "^1.26.1",
    "proj4-fully-loaded": "^0.2.0",
    "reproject-bbox": "^0.13.1",
    "srvd": "^0.6.0",
    "write-image": "^0.2.0"
