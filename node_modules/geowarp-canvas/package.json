{
  "name": "geowarp-canvas",
  "version": "0.2.0",
  "description": "Adds HTML Canvas Support to GeoWarp",
  "main": "./index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -fr ./test-output/*",
    "format": "npx prettier --arrow-parens=avoid --print-width=160 --trailing-comma=none --write *.js",
    "setup": "cd test-data && ./setup.sh",
    "test": "LOG_SKIP=false TEST_TIMED=true node test.js"
